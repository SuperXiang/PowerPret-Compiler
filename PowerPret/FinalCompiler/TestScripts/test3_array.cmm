 */

double[6] doubleArray; 
int[2]  intArray ; 
double a;
a = 2.0;
int b;
b = 0;


doubleArray[b] = 2.0;
doubleArray[1] = a;
doubleArray[2] = intArray[0];
a = 0.9;
doubleArray[3] = a;
doubleArray[4] = 5.0 - 2.0 / (4.0 - 3.0) * 2.5 + 0.01; 


read(doubleArray[5]);



int x;
x = 0;
while(x < 6)
{
	write(doubleArray[x]);
	x = x + 1;
}

